{
  "extends": [
    "next/core-web-vitals",
    "next",
    "prettier",
    "plugin:testing-library/react",
    "plugin:jest-dom/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "semi": [2, "always"],
    "comma-dangle": ["error", "always-multiline"],
    "prettier/prettier": ["error", { "singleQuote": true}]
  }
}